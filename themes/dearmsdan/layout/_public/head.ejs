<%# head头部信息 %>
  <%
  var title = page.title;
  var logo = "data:image/png;base64,";
  if (theme.head.favicon.href) {
    logo = theme.head.favicon.href;
  }
  if (is_archive()){
    title = __('归档');

    if (is_month()){
      title += '：' + page.year + '/' + page.month;
    } else if (is_year()){
      title += '：' + page.year;
    }
  } else if (is_category()){
    title = __('文章分类') + '：' + page.category;
  } else if (is_tag()){
    title = __('标签') + '：' + page.tag;
  }


  %>

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="keywords" content="<%= theme.head.keywords %>" />
	<meta name="description" content="<%= theme.head.description %>">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
	<title><% if (title){ %><%= title %> - <% } %> <%= config.title %></title>
	<link rel="shortcut icon" href="<%= logo %>" type="<%= theme.head.favicon.type %>" />
	<!-- 引入配置css文件 -->
	<%- css('css/seablueUI.min.css') %>
	<!-- 引入配置js文件 -->
	<%#- js('SeablueUI.js') %>
</head>





